def checkio(data):
    import re
    if len(data)> 9 and bool(re.search(r'\d', data)) is True:
        for i in data:
            if i.isalpha() is True:
                #if i.isdigit() is True:
              
                return True
  
           # if True:
            #    re.search('[A-Z]', data)
            #    if True:
             #       re.search('[0-9]', data)
            else:
                return False
    else:
        return False
